{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "A ReactJS boilerplate template",
  "main": "index.js",
  "scripts": {
    "build": "NODE_PATH=src webpack --config webpack.prod.js",
    "lint": "./node_modules/.bin/eslint . --fix",
    "start": "NODE_PATH=src webpack-dev-server --config webpack.dev.js",
    "test": "NODE_PATH=src jest",
    "test:watch": "NODE_PATH=src jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolencki13/react-boilerplate.git"
  },
  "keywords": [
    "reactjs",
    "boilerplate",
    "webpack",
    "react",
    "web"
  ],
  "author": "Brian Olencki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bolencki13/react-boilerplate/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/enzyme.setup.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "homepage": "https://github.com/bolencki13/react-boilerplate#readme",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "fetch-mock": "^6.4.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.4.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
