{
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "env": {
    "jest": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "fetch": true,
    "FormData": true,
    "FileReader": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
